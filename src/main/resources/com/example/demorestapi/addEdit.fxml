<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="parent" alignment="CENTER" prefHeight="680" prefWidth="480" spacing="20.0" stylesheets="@css.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demorestapi.AddEditController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label alignment="CENTER" text="DODA / UREDI" textFill="WHITE">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
      </Label>
      <Pane id="loginPane" prefHeight="680.0" prefWidth="440.0">
         <children>
            <Label alignment="CENTER" text="IME" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="name" alignment="CENTER" layoutY="20.0" prefHeight="25.0" prefWidth="440.0" />
            <Button fx:id="submit" alignment="CENTER" layoutX="366.0" layoutY="538.0" mnemonicParsing="false" onMouseClicked="#submit" text="POTRDI">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="cancel" alignment="CENTER" layoutX="286.0" layoutY="538.0" mnemonicParsing="false" onMouseClicked="#cancel" text="PREKLIÄŒI">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Label alignment="CENTER" layoutX="1.0" layoutY="53.0" text="OPIS" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextArea fx:id="description" layoutY="72.0" prefHeight="454.0" prefWidth="440.0" />
         </children></Pane>
   </children>
</VBox>
